
[loggers]
keys=root

[handlers]
keys=fileHandler

[formatters]
keys=simpleFormatter

[logger_root]
level=DISABLED
handlers=fileHandler

[handler_fileHandler]
class=FileHandler
level=NOTSET
formatter=simpleFormatter
args=('tmp-file.log',)

[formatter_simpleFormatter]
format=%(asctime)s %(name)s - %(levelname)s:%(message)s

[cli arg level override]
handler keys=fileHandler
