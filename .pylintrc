
[MASTER]
good-names=_,d,db,ex,f,i,id,j,k,v,x,y
init-hook='from pylint.config import find_pylintrc;
        import os, sys;
        sys.path.append(os.path.dirname(find_pylintrc()));
        sys.path.append(os.path.join(
                        os.path.dirname(find_pylintrc()), 'grand_trade_auto'));
        sys.path.append(os.path.join(
                        os.path.join(os.path.dirname(find_pylintrc()),
                                'grand_trade_auto'), 'general'));'

load-plugins=ci_support.trailing_commas
max-args=10


[MESSAGE CONTROL]
disable=
        logging-fstring-interpolation,
        logging-not-lazy,


[SIMILARITIES]
min-similarity-lines=8


[trailing-comma]
ignore-multi-line-if=yes
ignore-multi-line-tuple=no
ignore-function-def=yes
